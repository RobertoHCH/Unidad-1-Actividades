@inject Actividad_5_Animales.Services.ClaseService servicio
<html>
<head>
    <title>ZooPlanet</title>
    <link href="StyleSheet.css" rel="stylesheet">
    <link href="font-awesome.min.css" rel="stylesheet">
</head>
<body>
    <header>
        <h1><a href="~/Home"><b>Zoo</b>Planet</a></h1>
        <nav>
            <ul>
                <li><a href="~/Home">Home</a></li>
                <li>
                    <a href="#">Clasificación</a>
                    <ul>
                        @foreach (var serv in servicio.Clases)
                        {
                            <li><a href="~/@serv.Nombre">@serv.Nombre</a></li>
                        }
                    </ul>
                </li>
            </ul>
        </nav>
    </header>

    <main>
        @RenderBody()
    </main>

    <footer>Programación Web</footer>
</body>
</html>